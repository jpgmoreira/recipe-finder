{{!-- Pagination menu to navigate search result pages. --}}
<nav aria-label="Page navigation" class="d-flex justify-content-center">
	<ul class="pagination m-0">
		{{#each pagination}}
			{{#if this.current}}
				<li class="page-item active" aria-current="page">
					<span class="page-link">
						{{this.text}}
						<span class="sr-only">(current)</span>
					</span>
				</li>
			{{else}}
				<li class="page-item {{#if this.blocked}}disabled{{/if}}">
					<a class="page-link" href={{this.dest}}>
						{{this.text}}
					</a>
				</li>
			{{/if}}
		{{/each}}
	</ul>
</nav>
